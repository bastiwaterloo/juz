    "react-simple-maps": "^0.10.1",
    "react-uikit-button": "^2.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.2",
    "redux-promise-middleware": "^4.2.0",
    "redux-thunk": "^2.2.0",
    "scss": "0.2.4"

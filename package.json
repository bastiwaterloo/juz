    "google-maps-react": "^1.1.2",
    "isomorphic-fetch": "^2.2.1",
    "node-jsx": "^0.13.3",
    "prop-types": "15.5.8",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.6.0",
    "react-adsense": "0.0.2",
    "react-bootstrap": "0.31.0",
    "react-calendar-component": "^2.0.0",
    "react-ckeditor-wrapper": "^1.0.22",
    "react-cookie": "^2.0.7",
    "react-dom": "^15.4.2",
    "react-event-calendar": "^0.3.0",
    "react-fine-uploader": "^1.0.2",
    "react-firebase": "^2.2.8",
